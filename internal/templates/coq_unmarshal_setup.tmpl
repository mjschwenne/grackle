  << $refs := (filterByCoqType .Fields "go_string|u8|message|enum") ->>
  << $psls := (repeatedFields .Fields) ->>
  << if or (len $psls) (len $refs)  ->>
  unfold has_encoding in Henc.
  destruct Henc as (
  <<- range $idx, $r := (extEncFields .Fields) ->>
    << .Name >>_enc & << end ->>
    Henc
    <<- range .Fields >>
    <<- if or (isGoType . "message|enum") (isPredSliceType .) >> & Henc_<< .Name >>
    <<- if isPredSliceType . >> & H<< .Name >>_sz <<- end ->>
    <<- else if isCoqType . "go_string|u8" >> & Hlen_<< .Name >>
    <<- end >>
    <<- end >> ).
  << end ->>
  rewrite Henc. rewrite -?app_assoc.
