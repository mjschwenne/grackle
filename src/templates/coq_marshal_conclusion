
  wp_load. iApply "HÎ¦". iModIntro. rewrite -?app_assoc.
  iFrame. iPureIntro.

  << $refs := (refFields .Fields) ->>
  << if gt (len $refs) 0 ->>
  unfold has_encoding. exists << range $idx, $field := $refs ->>
  <<- if $idx >>, << end ->>
  <<- .Name ->>_enc
  <<- end ->>. << range $refs >>
  split. { exact. }
  <<- end >> { exact. }
  <<- else >> done.
  <<- end >>
Qed.
